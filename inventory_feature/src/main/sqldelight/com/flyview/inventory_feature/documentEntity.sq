CREATE TABLE documentEntity (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    number TEXT NOT NULL
);

insert:
INSERT INTO documentEntity (number)
VALUES (?);

deleteAll:
DELETE FROM documentEntity;

countDocuments:
SELECT count(*)
FROM documentEntity;

document:
SELECT *
FROM documentEntity
WHERE
    id = ?;

documents:
SELECT *
FROM documentEntity
LIMIT :limit OFFSET :offset;

lastInsertRowId:
SELECT last_insert_rowid();